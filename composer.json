{
    "description": "Symfony UX Reproducer - Repository template",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Simon AndrÃ©",
            "email": "smn.andre@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/asset": "7.2.*",
        "symfony/asset-mapper": "7.2.*",
        "symfony/console": "7.2.*",
        "symfony/dotenv": "7.2.*",
        "symfony/flex": "^2.4.7",
        "symfony/form": "7.2.*",
        "symfony/framework-bundle": "7.2.*",
        "symfony/http-foundation": "7.2.*",
        "symfony/http-kernel": "7.2.*",
        "symfony/monolog-bundle": "^3.10",
        "symfony/routing": "7.2.*",
        "symfony/runtime": "7.2.*",
        "symfony/twig-bundle": "7.2.*",
        "symfony/ux-icons": "^2.22.1",
        "symfony/ux-live-component": "^2.22.1",
        "symfony/ux-turbo": "^2.22",
        "symfony/ux-twig-component": "^2.22.1",
        "symfony/validator": "7.2.*",
        "symfony/yaml": "7.2.*",
        "twig/twig": "^3.18"
    },
    "require-dev": {
        "symfony/debug-bundle": "7.2.*",
        "symfony/stopwatch": "~7.2.2",
        "symfony/web-profiler-bundle": "~7.2.2"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        },
        "thanks": {
            "name": "symfony/ux",
            "url": "https://github.com/symfony/ux"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
